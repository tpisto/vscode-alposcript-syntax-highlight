{
  "name": "vscode-alposcript-syntax-highlighting",
  "version": "0.1.10",
  "publisher": "tpisto",
  "repository": "https://github.com/tpisto/vscode-alposcript-syntax-highlighting.git",
  "displayName": "AlpoScript Syntax Highlighting",
  "keywords": [
    "alposcript",
    "AlpoScript"
  ],
  "engines": {
    "vscode": "*"
  },
  "categories": [
    "Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "alposcript",
        "extensions": [
          ".as",
          ".asx"
        ],
        "aliases": [
          "AlpoScript",
          "alposcript"
        ],
        "configuration": "./alposcript.configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "alposcript",
        "scopeName": "source.alposcript",
        "path": "./syntaxes/AlpoScript.tmLanguage"
      }
    ],
    "debuggers": [
      {
        "type": "node",
        "enableBreakpointsFor": {
          "languageIds": [
            "alposcript"
          ]
        }
      }
    ]
  }
}